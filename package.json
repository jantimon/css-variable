{
  "name": "css-variable",
  "version": "4.0.3",
  "description": "define CSS custom properties (variables) in JS",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./babel": "./babel/index.js",
    "./swc": "./swc/target/wasm32-wasi/release/swc_plugin_css_variable.wasm"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "babel",
    "swc/package.json",
    "swc/target/wasm32-wasi/release/swc_plugin_css_variable.wasm"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build && npm run build:swc",
    "build": "npm run build:types && npm run build:commonjs && npm run build:module && npm run build:modulemin",
    "build:commonjs": "babel --config-file=./babel.commonjs.cjs -o dist/index.js src/index.ts",
    "build:module": "babel --config-file=./babel.config.cjs -o dist/index.mjs src/index.ts",
    "build:types": "tsc --skipLibCheck --emitDeclarationOnly --declaration --target ESNext --outDir dist src/index.ts",
    "build:modulemin": "terser ./dist/index.mjs -o ./dist/index.min.mjs -m --ecma 2017 --module --toplevel -b -c",
    "build:swc": "cargo build --manifest-path ./swc/Cargo.toml --release --target=wasm32-wasi",
    "changelog": "npx standard-version",
    "test": "npm run test:e2e && npm run test:vitest",
    "test:e2e": "node --experimental-modules ./test/examples.js",
    "test:vitest": "vitest run",
    "test:swc": "npm run test:swc:cargo && npm run build:swc && npm run test:swc:generate-config && npm run test:swc:transform && npm run test:swc:vitest",
    "test:swc:generate-config": "node ./test/swc/generate-swcrc.js",
    "test:swc:transform": "swc ./test/swc/swc.test.js -o ./test/swc/swc.test.transformed.js --config-file ./test/swc/.swcrc",
    "test:swc:vitest": "vitest run -c ./test/swc/vitest.config.js",
    "test:swc:cargo": "cargo test --manifest-path ./swc/Cargo.toml",
    "docs": "node ./docs/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jantimon/css-variable.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "css"
  ],
  "author": "Jan Nicklas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jantimon/css-variable/issues"
  },
  "homepage": "https://css-variable.js.org/",
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.22.0",
    "@babel/plugin-transform-modules-commonjs": "7.19.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime": "7.20.1",
    "@babel/types": "7.22.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.4.17",
    "@swc/types": "0.1.12",
    "@types/node": "20.0.0",
    "terser": "5.15.1",
    "typescript": "5.4.5",
    "vitest": "0.34.0"
  }
}
